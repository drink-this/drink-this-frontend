{
  "name": "drink-this",
  "version": "0.0.1",
  "description": "An App that lets users rate drinks and get recommendations on how they rate other drinks",
  "main": "index.js",
  "repository": "git@github.com:drink-this/drink-this-frontend.git",
  "license": "MIT",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "test": "cross-env NODE_ENV=test jest",
    "watch": "npm run development -- --watch",
    "server": "node ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "flux": "^4.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^27.0.6",
    "jquery": "^3.6",
    "webpack": "^5.47.0",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/js/"
    ],
    "testRegex": "js/test/.*.test.js$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  }
}
